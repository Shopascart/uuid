{
    "name": "@shopascart/uuid",
    "version": "1.2.3",
    "description": "A uuid generator",
    "main": "dist/cjs/index.js",
    "module": "dist/ts/index.js",
    "files": [
        "dist"
    ],
    "directories": {
        "dist": "dist"
    },
    "scripts": {
        "build": "rm -rf dist && npm run build:ts && npm run build:cjs",
        "build:ts": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "prepublishOnly": "npm run build",
        "tsc:init": "tsc --init"
    },
    "keywords": [],
    "author": "Shopascart",
    "devDependencies": {
        "typescript": "^4.7.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Shopascart/uuid.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Shopascart/uuid/issues"
    },
    "homepage": "https://github.com/Shopascart/uuid/#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}
